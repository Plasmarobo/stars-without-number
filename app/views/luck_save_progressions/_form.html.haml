= form_for @luck_save_progression do |f|
  - if @luck_save_progression.errors.any?
    #error_explanation
      %h2= "#{pluralize(@luck_save_progression.errors.count, "error")} prohibited this luck_save_progression from being saved:"
      %ul
        - @luck_save_progression.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :character_class_id
    = f.number_field :character_class_id
  .field
    = f.label :level
    = f.number_field :level
  .field
    = f.label :luck_save
    = f.number_field :luck_save
  .actions
    = f.submit 'Save'
