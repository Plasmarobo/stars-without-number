= form_for @ship_share do |f|
  - if @ship_share.errors.any?
    #error_explanation
      %h2= "#{pluralize(@ship_share.errors.count, "error")} prohibited this ship_share from being saved:"
      %ul
        - @ship_share.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :character_id
    = f.number_field :character_id
  .field
    = f.label :ship_id
    = f.number_field :ship_id
  .field
    = f.label :shares
    = f.number_field :shares
  .actions
    = f.submit 'Save'
