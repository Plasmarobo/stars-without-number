= form_for @readied_item_slot do |f|
  - if @readied_item_slot.errors.any?
    #error_explanation
      %h2= "#{pluralize(@readied_item_slot.errors.count, "error")} prohibited this readied_item_slot from being saved:"
      %ul
        - @readied_item_slot.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :character_id
    = f.number_field :character_id
  .field
    = f.label :item_slot_id
    = f.number_field :item_slot_id
  .actions
    = f.submit 'Save'
